# class Solution:
#     def findMin(self, nums: List[int]) -> int:
#         minimum = min(nums)

#         return minimum
        

class Solution:
    def findMin(self, nums: List[int]) -> int:

        left = 0
        right = len(nums) - 1

        while left < right:

            mid = (left + right) // 2

            # minimum is in right half
            if nums[mid] > nums[right]:
                left = mid + 1

            # minimum is in left half including mid
            else:
                right = mid

        return nums[left]
